config_files=($HOME/i3wm/zsh)
# Plugins# {{{
source $config_files/plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
source $config_files/plugins/shell-plugins/zsh-git/git.plugin.zsh
source $config_files/plugins/zsh-autopair/autopair.zsh
# }}}
# Config Files{{{
source $config_files/prompt.zsh
source $config_files/config.zsh
source $config_files/aliases.zsh
source $config_files/commands.zsh
source $config_files/completion.zsh
# }}}
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
